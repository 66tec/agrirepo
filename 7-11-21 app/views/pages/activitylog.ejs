<body>
    <!--  wrapper -->
    <%- include('../templates/leftnavbarsuper.ejs') %>
    <div class="page">
      <!-- navbar-->
      <header class="header">
        <%- include('../templates/topbar.ejs') %>
      </header>
      <!-- Counts Section -->
      <!-- Header Section-->

      <div class="row">
        <!-- Welcome -->
        <div class="col-lg-12">
            <div class="alert alert-info">
                <i class="fa fa-folder-open"></i><b>&nbsp;Hello ! </b>Welcome Back <b><%= username_dash  %> </b>
            </div>
        </div>
        <!--end  Welcome -->
      </div>


      <div class="row">
        <div class="col-lg-12">



 
           

                <div class="panel-body container">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="table-responsive">
                                <table class="table table-bordered table-hover table-striped">
                                    <thead>
                                        <br>
                                        <center>
                                        <strong class="text-primary" style="font-size: 25px;">Activity Logs</strong></div>
                                    </center>
                                        <tr>
                                            <th>Activity#</th>
                                            <th>Date and time</th>
                                            <th>Activity Description</th>
                                            <th>Activity Type</th>
                                            <th>Chamber Name</th>
                                            
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% stream.forEach(function(item) { %>
                                        <tr>
                                            <td> <%= item.activity_id%></td>
                                            <td> <%= item.activity_time%></td>
                                            <td> <%= item.activity_desc%></td>
                                            <td> <%= item.activity_type%></td>
                                            <td> <%= item.chamber_name%></td>
                                            
                                        </tr>
                                        <% }); %>
                                       

                                    </tbody>
                                </table>
                            </div>

                        </div>

                    </div>
                    <!-- /.row -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!--End simple table example -->

        </div>
       
            <!--End Chat Panel Example-->
            <%- include('../templates/copyright.ejs') %>
    </div>

   
    <!-- JavaScript files-->
    <script src="vendor/jquery/jquery.min.js"></script>
