      <div class="row">
        <!-- Welcome -->
        <div class="col-lg-12">
          <div class="alert alert-info">
            <i class="fa fa-folder-open"></i><b>&nbsp;Hello ! </b>Welcome Back <b>
              <%= username_dash %>
            </b>
          </div>
        </div>
        <!--end  Welcome -->
      </div>
      <!-- Counts Section -->
      <%- include('../templates/topnotices.ejs') %>
        <!-- Header Section-->

        <!-- <div class="row">
          <div class="col-lg-1"></div>
          <div class="col-md-12 col-sm-12 col-lg-5" style="margin-left: 2px;"> -->
            <center>
              <h1>Your Gallery</h1>
  
     
              <div id="carouselId" class="carousel slide container" data-ride="carousel">
                <ol class="carousel-indicators">
                  <li data-target="#carouselId" data-slide-to="0" class="active"></li>
                  <% tfa.forEach(function(item) { %>
                  <li data-target="#carouselId" data-slide-to="0" class="active"></li>
                  <% }); %>
                </ol>
                <div class="carousel-inner" role="listbox" style="border: solid black 5px;">
                  <div class="carousel-item active">
                    <img src="indeximages/v3.jpg" alt="First slide" width="840" height="560">
                    <div class="carousel-caption">
                      <h3>Muzammil farm</h3>
                    </div>
                    
                  </div>

                  <% tfa.forEach(function(item) { %>
                  <div class="carousel-item">
                    <img src="<%= item.path%>" alt="First slide" width="840" height="560">
                    <div class="carousel-caption">
                      <h3><%= item.username%> farm</h3>
                    </div>
                  </div>
                  <% }); %>

                  <!-- <div class="carousel-item">
                    <img src="indeximages/v2.jpg" alt="Second slide">
                  </div>
                  <div class="carousel-item">
                    <img src="indeximages/v2.jpg" alt="Third slide">
                  </div> -->
                </div>
                <a class="carousel-control-prev" href="#carouselId" role="button" data-slide="prev">
                  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                  <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselId" role="button" data-slide="next">
                  <span class="carousel-control-next-icon" aria-hidden="true"></span>
                  <span class="sr-only">Next</span>
                </a>
              </div>

            </center>
          <!-- </div> -->

          <!-- <div class="col-md-12 col-sm-12 col-lg-5" style="margin-left: 2px;"> -->
            <center>
              <h1>Timeline</h1>
              <div class="container">
                <div class="carousel-inner" role="listbox" style="border: solid black 5px;">
                    <div class="carousel-item active">
                      <img src="indeximages/timeline.jpg" alt=""  width="840" height="560">
                    </div>
                </div></div>
            </center>
          <!-- </div> -->

          







